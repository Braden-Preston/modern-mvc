{
  "name": "prisma-vite",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:reload": "livereload -ee 'ts,hbs' -w 100  src",
    "dev:client": "vite build -w -c src/vite.config.js",
    "dev:server": "vite",
    "build": "run-s build:*",
    "build:client": "vite build -c src/vite.config.js",
    "build:server": "tsc && vite build",
    "preview": "run-s build serve",
    "serve": "node dist/server"
  },
  "dependencies": {
    "fastify": "^3.28.0",
    "fastify-static": "^4.6.1",
    "handlebars": "^4.7.7",
    "htmx.org": "^1.7.0",
    "point-of-view": "^5.2.0"
  },
  "devDependencies": {
    "@swc/core": "^1.2.165",
    "@types/livereload": "^0.9.2",
    "autoprefixer": "^10.4.4",
    "connect-livereload": "^0.6.1",
    "desm": "^1.2.0",
    "livereload": "^0.9.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.5.4",
    "vite": "^2.9.4",
    "vite-plugin-full-reload": "^1.0.0",
    "vite-plugin-node": "^0.0.19"
  }
}
